<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="registry_report_view" model="ir.ui.view">
            <field name="name">registry.report.form</field>
            <field name="model">report.registry</field>
            <field name="arch" type="xml">
                <form string="">
                    <footer>
                        <button name="print_report" string="Imprimir" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_report_contract_wizard" model="ir.actions.act_window">
            <field name="res_model">report.registry</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="registry_report_view"/>
            <field name="target">new</field>
        </record>

        <!-- <menuitem
            id="menu_wizard"
            name="wizard"
            action="action_report_contract_wizard"
            parent="menu_registry"
            sequence="3"/> -->
        
    </data>
</odoo>
